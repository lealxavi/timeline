// More information about manifest -> https://developer.chrome.com/extensions/getstarted#declaration
// How to move on -> https://developer.chrome.com/extensions/getstarted#next-steps

{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click me!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}

